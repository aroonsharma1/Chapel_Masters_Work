#!/usr/bin/env bash

CWD=$(cd $(dirname $0) ; pwd)

source $CWD/common-perf.bash

log_info "Testing fifo performance."
$CWD/nightly -cron -performance -releasePerformance -numtrials 5 -startdate 07/28/12

log_info "Testing qthreads performance."
source $CWD/common-qthreads.bash
$CWD/nightly -cron -execopts --dataParTasksPerLocale=8 -performance-description 'qthreads --genGraphOpts "-m default -m qthreads"' -numtrials 5 -startdate 07/28/12

log_info "Finished ${0}."
