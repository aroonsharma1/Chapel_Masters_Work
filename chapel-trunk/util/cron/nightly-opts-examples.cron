#!/usr/bin/env bash
echo `date` "" Starting $0 on `hostname`.

LOGFILE=$HOME/tmp/nightly.opts.out
echo `date` "" "Testing -cron.  Log file: $LOGFILE"
export CHPL_HOME_REPOSITORY=svn://svn.code.sf.net/p/chapel/code/trunk && ~/chapel/util/cron/start_opt_test -cron >& $LOGFILE

echo `date` "" Finished $0.
