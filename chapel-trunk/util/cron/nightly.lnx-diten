#!/bin/tcsh
echo `date` "" Starting $0 on `hostname`.

# run cyclic dist regressions
set LOGFILE = $HOME/tmp/nightly.cyclic.out
echo `date` "" "Testing cyclic distribution.  Log file: $LOGFILE"
source ~/.chplcron.dists.cshrc && ~/chapel/util/cron/nightly -cron -dist cyclic >&! $LOGFILE

echo `date` "" Finished $0.
