all: histogram life MonteCarloPi boundedBuffer

histogram:
	pdflatex -file-line-error -halt-on-error histogram.tex

life:
	pdflatex -file-line-error -halt-on-error life.tex

boundedBuffer:
	pdflatex -file-line-error -halt-on-error boundedBuffer.tex

MonteCarloPi:
	pdflatex -file-line-error -halt-on-error MonteCarloPi.tex

clean:
	rm -f *.aux *.blg *.bbl *.dvi *.log
