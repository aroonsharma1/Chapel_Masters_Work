set style data histogram
set style histogram cluster gap 2
set style fill solid border rgb "black"
set auto x
set xtics nomirror rotate -45 font ",8"
set xlabel "Benchmark"
set ylabel "Message Count Normalized to Block Cyclic Distribution"
set yrange [0:1.2]
plot 'block_cyclic_message_count.dat' using 2:xtic(1) title col,'' using 3:xtic(1) title col
