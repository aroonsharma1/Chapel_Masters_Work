#!/bin/tcsh
echo Starting $0 on `hostname`.; module list; printenv; echo

set LOGFILE = $HOME/tmp/nightly.linux64.out
echo "Testing the default-everything configuration on `date`.  Log file: $LOGFILE"
source ~/.chapelcshrc-cron && ~/chapel/util/cron/nightly -cron >&! $LOGFILE

echo Finished $0 on `date`.
