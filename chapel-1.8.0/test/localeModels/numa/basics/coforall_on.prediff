#!/usr/bin/env python

import sys;

logfile = sys.argv[2]

f = open(logfile)
lines = f.readlines()
f.close()

newlines = list()
sorted = list()
count = 0
numEntries = 5
for l in lines:
    if l != '\n':
        count += 1
        sorted.append(l)
        if count == numEntries:
            sorted.sort()
            newlines += sorted
            count = 0
            for i in range(0,numEntries):
                sorted.pop()

newlines += sorted

f = open(logfile, 'w')
count = 0
for l in newlines:
    count += 1
    f.write('%s'%(l))
    if count == numEntries:
        count = 0
        f.write('\n')

f.close()

sys.exit(0)

